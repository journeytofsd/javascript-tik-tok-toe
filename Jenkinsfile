pipeline {
    agent any
    stages {
      stage('Example Checkout') {
            steps {
                echo 'Executing the stage: Build'
            }
        }
        stage('Example Build') {
            steps {
                echo 'Executing the stage: Build'
            }
        }
      stage('Example Test') {
            steps {
               echo 'Executing the stage: Test'
            }
        }
      stage('Example Deploy') {
            steps {
                 echo 'Executing the stage: Deploy'
            }
        }
    }
}
